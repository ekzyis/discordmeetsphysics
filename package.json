{
  "name": "physicsbot",
  "version": "1.0.0",
  "description": "A Discord bot intended for usage with a physics discord server",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=production node lib/bot.js",
    "dev": "npm run build && NODE_ENV=development node lib/bot.js",
    "build": "rm -rf lib/ && babel src/ -d lib/",
    "pidev": "cd ~/physicstest && npm run dev",
    "pistart": "cd ~/physicsbot && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ekzyis/physicsbot.git"
  },
  "author": "ekzyis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ekzyis/physicsbot/issues"
  },
  "homepage": "https://github.com/ekzyis/physicsbot#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "discord.js": "^11.4.2",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "prettier": "^1.15.3"
  }
}
